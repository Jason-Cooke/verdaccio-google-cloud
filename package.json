{
  "name": "verdaccio-google-cloud",
  "version": "0.0.9",
  "description": "verdaccio storage plugin implementation for google cloud",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "yarn lint",
    "test": " jest --config ./jest.config.js --maxWorkers 2",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma none --write \"{src,types,test}/**/*.{ts}\"",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "build": "babel src/ --out-dir lib/ --copy-files --ignore ___tests___",
    "coverage:publish": "codecov",
    "release": "standard-version -a -s"
  },
  "keywords": [
    "verdaccio",
    "verdaccio-plugin",
    "google-cloud"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio-google-cloud"
  },
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "@google-cloud/storage": "^2.3.4",
    "@verdaccio/local-storage": "^2.1.0",
    "@verdaccio/streams": "^1.0.0",
    "fast-crc32c": "^1.0.4",
    "http-errors": "1.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/travis-cli": "7.0.0",
    "@types/google-cloud__datastore": "^1.3.3",
    "@types/http-errors": "1.6.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.12.1",
    "@verdaccio/babel-preset": "0.1.0",
    "@verdaccio/eslint-config": "^0.0.1",
    "@verdaccio/types": "5.0.0-beta.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "codecov": "3.2.0",
    "cross-env": "5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "0.9.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "0.14.3",
    "jest": "24.5.0",
    "lint-staged": "7.2.0",
    "memory-fs": "^0.4.1",
    "prettier": "1.13.7",
    "rmdir-sync": "1.0.1",
    "standard-version": "5.0.2",
    "typescript": "3.2.4"
  },
  "engines": {
    "node": ">=8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js, json}": [
      "eslint .",
      "prettier --write",
      "git add"
    ]
  }
}
