{
  "name": "verdaccio-google-cloud",
  "version": "0.0.9",
  "description": "verdaccio storage plugin implementation for google cloud",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "yarn lint",
    "test": " jest --config ./jest.config.js --maxWorkers 2",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma none --write \"{src,types,test}/**/*.js\"",
    "lint": "npm run flow && eslint .",
    "flow": "flow",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "build": "babel src/ --out-dir lib/ --copy-files --ignore ___tests___",
    "coverage:publish": "codecov",
    "release": "standard-version -a -s"
  },
  "keywords": [
    "verdaccio",
    "verdaccio-plugin",
    "google-cloud"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio-google-cloud"
  },
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/datastore": "^3.1.2",
    "@google-cloud/storage": "^2.4.3",
    "@verdaccio/streams": "^2.0.0",
    "fast-crc32c": "^1.0.4",
    "http-errors": "1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/travis-cli": "7.5.2",
    "@types/aws-sdk": "2.7.0",
    "@types/http-errors": "1.6.1",
    "@types/jest": "24.0.11",
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "@verdaccio/babel-preset": "0.1.0",
    "@verdaccio/types": "5.0.0-beta.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.4.0",
    "codecov": "^3.0.4",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-google": "0.12.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "0.14.3",
    "jest": "24.5.0",
    "lint-staged": "7.2.0",
    "memory-fs": "0.4.1",
    "prettier": "1.16.4",
    "rmdir-sync": "1.0.1",
    "standard-version": "5.0.2"
  },
  "engines": {
    "node": ">=6.12.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js, json}": [
      "eslint .",
      "prettier --write",
      "git add"
    ]
  }
}
